{
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "pretty": true,
    "types": [
      "node"
    ],
    "declaration": true,
    "sourceMap": false,
    "target": "es6",
    "baseUrl": "./",
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules",
    "test"
  ]
}